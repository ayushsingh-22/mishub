"text":["\u001b[1m138/138\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m28s\u001b[0m 202ms/step\n","Predictions saved to /kaggle/working/goa.xlsx\n"]}],"source":["import os\n","import pandas as pd\n","from tensorflow.keras.preprocessing.image import ImageDataGenerator\n","import tensorflow as tf\n","\n","# Directory for the test images\n","test_base_dir = '/kaggle/input/dataset/Testing set/Images/'\n","\n","# Create a DataFrame with only valid image files\n","valid_extensions = {'.jpg', '.jpeg', '.png'}\n","test_image_files = [f for f in os.listdir(test_base_dir) if os.path.splitext(f)[1].lower() in valid_extensions]\n","test_df = pd.DataFrame(test_image_files, columns=['image_filename'])\n","\n","# Construct image path column for generator input and output\n","test_df['image_path'] = test_df['image_filename'].apply(lambda x: os.path.join(test_base_dir, x))\n","\n","# Data augmentation for test images (only rescaling)\n","val_datagen = ImageDataGenerator(rescale=1./255)\n","\n","# Test data generator\n","test_generator = val_datagen.flow_from_dataframe(\n","    dataframe=test_df,\n","    x_col='image_path',  # Column with image paths\n","    y_col=None,  # No labels for test data\n","    target_size=(224, 224),\n","    batch_size=32,\n","    class_mode=None,\n","    shuffle=False\n",")\n","\n","# Load the trained model\n","model = tf.keras.models.load_model('/kaggle/working/final_modelvgg16.keras')\n","\n","# Generate predictions\n","y_pred = model.predict(test_generator)\n","\n","# Define class labels\n","class_labels = ['Angioectasia', 'Bleeding', 'Erosion', 'Erythema', 'Foreign Body', \n","                'Lymphangiectasia', 'Normal', 'Polyp', 'Ulcer', 'Worms']\n","\n","# Create DataFrame for predictions\n","predicted_df = pd.DataFrame(y_pred, columns=class_labels)\n","predicted_df['predicted_class'] = predicted_df.idxmax(axis=1)\n","\n","# Include image_path in the final DataFrame for output\n","final_df = pd.concat([test_df[['image_filename']], predicted_df], axis=1)\n","\n","# Rename 'image_filename' to 'image_path' in the final output\n","final_df = final_df.rename(columns={'image_filename': 'image_path'})\n","\n","# Save to Excel\n","output_file = '/kaggle/working/goa.xlsx'\n","final_df.to_excel(output_file, index=False)\n","print(f\"Predictions saved to {output_file}\")\n"]},{"cell_type":"code","execution_count":5,"id":"0059254a","metadata":{"execution":{"iopub.execute_input":"2024-10-27T14:28:50.890077Z","iopub.status.busy":"2024-10-27T14:28:50.889215Z","iopub.status.idle":"2024-10-27T15:03:18.387371Z","shell.execute_reply":"2024-10-27T15:03:18.386067Z"},"papermill":{"duration":2071.020589,"end_time":"2024-10-27T15:03:20.2563","exception":false,"start_time":"2024-10-27T14:28:49.235711","status":"completed"},"tags":[]},"outputs":[{"data":{"text/html":["<pre style=\"white-space:pre;overflow-x:auto;line-height:normal;font-family:Menlo,'DejaVu Sans Mono',consolas,'Courier New',monospace\"><span style=\"font-weight: bold\">Model: \"functional_1\"</span>\n","</pre>\n"],"text/plain":["\u001b[1mModel: \"functional_1\"\u001b[0m\n"]},"metadata":{},"output_type":"display_data"},{"data":{"text/html":["<pre style=\"white-space:pre;overflow-x:auto;line-height:normal;font-family:Menlo,'DejaVu Sans Mono',consolas,'Courier New',monospace\">┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┓\n","┃<span style=\"font-weight: bold\"> Layer (type)                    </span>┃<span style=\"font-weight: bold\"> Output Shape           </span>┃<span style=\"font-weight: bold\">       Param # </span>┃\n","┡━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━┩\n","│ input_layer (<span style=\"color: #0087ff; text-decoration-color: #0087ff\">InputLayer</span>)        │ (<span style=\"color: #00d7ff; text-decoration-color: #00d7ff\">None</span>, <span style=\"color: #00af00; text-decoration-color: #00af00\">224</span>, <span style=\"color: #00af00; text-decoration-color: